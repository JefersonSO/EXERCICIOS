algoritmo "maiorvalor"
// Função :
// Autor :
// Data : 30/06/2022
// Seção de Declarações 
var
  v1, v2, maior : Inteiro
inicio
  Escreval(" digite um valor : ")
  Leia(v1)
  escreval(" outro valor : " )
  Leia(v2)
  maior <- 0
  Se v1 > v2 entao
   maior <- v1
  senao
    maior <- v2
  fimSe
  Escreval(" O maior numero foi: ", maior)
fimalgoritmo
